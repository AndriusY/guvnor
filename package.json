{
  "name": "process-boss",
  "description": "A node process manager that isn't spanners all the way down",
  "version": "2.3.3",
  "prefer-global": true,
  "homepage": "https://github.com/tableflip/boss",
  "repository": {
    "type": "git",
    "url": "git://github.com/tableflip/boss.git"
  },
  "bugs": {
    "url": "https://github.com/tableflip/boss/issues"
  },
  "scripts": {
    "test": "istanbul cover _mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "postinstall": "chmod +x scripts/init/sysv/boss"
  },
  "dependencies": {
    "async": "^0.9",
    "boss-local": "^2.0",
    "boss-rc": "^0.5",
    "colors": "^1.0",
    "commander": "^2.0",
    "moment": "^2.0",
    "prettysize": "^0.0",
    "stackup": "^0.0",
    "posix": "^1.0",
    "wantsit": "^1.0",
    "wildemitter": "^1.0",
    "winston": "^0.8"
  },
  "devDependencies": {
    "coveralls": "^2.8",
    "istanbul": "^0.3.0",
    "mocha": "^2.0",
    "proxyquire": "^1.0",
    "chai": "^1.0",
    "sinon": "^1.8",
    "testsuite": "^1.0"
  },
  "bin": {
    "bs": "bin/bs",
    "boss": "bin/bs"
  },
  "keywords": [
    "node",
    "process",
    "manager",
    "monitor",
    "runner",
    "auto restart",
    "cluster",
